datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

generator client {
    provider    = "cargo prisma"
    output      = "../../crates/prisma/src/prisma.rs"
    module_path = "crate::prisma"
}

model User {
  id       Int      @id @default(autoincrement())
  email    String   @unique
  password String
}
